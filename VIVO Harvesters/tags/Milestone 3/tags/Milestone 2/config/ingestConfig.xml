<?xml version="1.0" encoding="UTF-8"?>
<Config>
  <Task id="fetchPubmedAll" type="fetch">
    <Schedule type="time">
      <Minute>0</Minute>
      <Hour>0</Hour>
      <DayOfMonth>*</DayOfMonth>
      <Month>*</Month>
      <DayOfWeek>0</DayOfWeek>
    </Schedule>
    <Server>
      <IPAddress>127.0.0.1</IPAddress>
      <CommandPort>12345</CommandPort>
      <InputPort>12346</InputPort>
      <OutputPort>12347</OutputPort>
    </Server>
    <Source type="repository">
      <Location>PubMed</Location>
      <Query>1:8000[dp]</Query>
    </Source>
    <Outputs>
      <Output type="file">XMLVault/FetchTest.xml</Output>
      <Output type="stream"><Destination>xsltPubmedAll</Destination></Output>
    </Outputs>
    <Params>
      <Email>cah@ichp.ufl.edu</Email>
      <Location>University of Florida</Location>
    </Params>
  </Task>
  <Task id="xsltPubmedAll" type="translate">
    <Schedule type="dependant">
      <Parent>fetchPubmedAll</Parent>
    </Schedule>
    <Server>
      <IPAddress>127.0.0.1</IPAddress>
      <CommandPort>12348</CommandPort>
      <InputPort>12349</InputPort>
      <OutputPort>12350</OutputPort>
    </Server>
    <Source type="stream">
      <Location>fetchPubmedAll</Location>
    </Source>
    <Outputs>
      <Output type="file">XMLVault/TransTest.xml</Output>
      <Output type="stream">scoreAll</Output>
    </Outputs>
    <Params>
      <Type>XSLTranslate</Type>
      <Source>DataMaps/PubMedToVivo.xsl</Source>
    </Params>
  </Task>
  <Task id="scoreAll" type="score">
    <Schedule type="dependant">
      <Parent>xsltPubmedAll</Parent>
    </Schedule>
    <Server>
    </Server>
    <Source type="stream">
      <Location>xsltPubmedAll</Location>
    </Source>
    <Outputs>
      <Output>
      </Output>
    </Outputs>
    <Params>
    </Params>
  </Task>
  <Task id="qualifyData" type="qualify">
    <Schedule>
    </Schedule>
    <Server>
    </Server>
    <Source>
    </Source>
    <Outputs>
      <Output>
      </Output>
    </Outputs>
    <Params>
    </Params>
  </Task>
  <Task id="transferToVIVO" type="transfer">
    <Schedule>
    </Schedule>
    <Server>
    </Server>
    <Source>
    </Source>
    <Outputs>
      <Output>
      </Output>
    </Outputs>
    <Params>
    </Params>
  </Task>
</Config>
